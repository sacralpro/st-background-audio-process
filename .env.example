# Пример файла .env для локального тестирования Appwrite функции
# Скопируйте этот файл в .env и заполните своими значениями

# Appwrite параметры
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_FUNCTION_PROJECT_ID=ваш_project_id
APPWRITE_API_KEY=ваш_api_key
APPWRITE_DATABASE_ID=ваш_database_id
APPWRITE_COLLECTION_ID=posts
APPWRITE_BUCKET_ID=ваш_bucket_id

# Дополнительные параметры (опционально)
APPWRITE_COLLECTION_ID_LOGS=logs
PROGRESS_WEBHOOK_URL=https://your-webhook-url.com/progress

# Параметры FFmpeg
# Оставьте пустыми, если ffmpeg доступен глобально
FFMPEG_PATH=
FFPROBE_PATH= 